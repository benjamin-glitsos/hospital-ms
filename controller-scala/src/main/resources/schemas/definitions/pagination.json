{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Pagination Definitions",
    "definitions": {
        "page_number": {
            "description": "The page of items to select.",
            "type": "integer",
            "minimum": 1
        },
        "page_length": {
            "description": "The number of items per page.",
            "type": "integer",
            "enum": [10, 25, 50, 100]
        }
    }
}
