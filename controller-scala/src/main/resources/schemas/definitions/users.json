{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "User Definitions",
    "definitions": {
        "username": {
            "description": "E.g. bdole43",
            "type": "string"
        },
        "email_address": {
            "description": "E.g. bdole@example.com",
            "type": "string"
        },
        "first_name": {
            "description": "E.g. Bob",
            "type": "string"
        },
        "last_name": {
            "description": "E.g. Dole",
            "type": "string"
        },
        "other_names": {
            "description": "Middle names and any other names that a person may possess. E.g. James William",
            "type": "string"
        },
        "password": {
            "description": "A strong, secure password.",
            "type": "string"
        }
    }
}
