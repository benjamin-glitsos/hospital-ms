{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Edit Users",
    "type": "object",
    "properties": {
        "username": {
            "type": "string",
            "maxLength": 20
        },
        "email_address": {
            "type": "string",
            "maxLength": 50
        },
        "first_name": {
            "type": "string",
            "maxLength": 50
        },
        "last_name": {
            "type": "string",
            "maxLength": 70
        },
        "other_names": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "maxLength": 120
        },
        "password": {
            "type": "string",
            "maxLength": 40
        },
        "additional_notes": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "maxLength": 1048576
        }
    },
    "minProperties": 1
}
