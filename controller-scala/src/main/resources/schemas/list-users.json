{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "List Users",
    "type": "object",
    "properties": {
        "page_number": {
            "type": "integer",
            "minimum": 1,
            "default": 1
        },
        "page_length": {
            "type": "integer",
            "enum": [10, 25, 50, 100],
            "default": 10
        },
        "search": {
            "type": "string",
            "minLength": 1,
            "maxLength": 4096,
            "default": ""
        }
    }
}
