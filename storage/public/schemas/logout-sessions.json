{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Logout Sessions",
  "type": "object",
  "properties": {
    "authentication_token": {
      "type": "string",
      "pattern": "^[_a-zA-Z0-9]{17}.[a-zA-Z0-9]{32}$",
      "patternDescription": "authentication_token is of the format: <metakey>.<session_token>",
      "minLength": 50,
      "maxLength": 50
    }
  },
  "required": [
    "authentication_token"
  ]
}
